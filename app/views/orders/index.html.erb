<h5>Orders for <%= current_user.name %>:</h5>


  <div class="row">
    <div class="m4">
      <div class="card grey darken-1 large">
        <% if @orders.empty? %>
          <div>No orders to show! <%= link_to "Browse", items_path %></div>
        <% end %>
        <ul>
          <% @orders.each do |order| %>
            <li><a><%= order.created_at %></a></li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
